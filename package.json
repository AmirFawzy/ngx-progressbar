{
  "name": "@ngx-progressbar/demo",
  "description": "A nanoscopic progress bar. Featuring realistic trickle animations to convince your users that something is happening!",
  "version": "4.3.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build-all": "npm run build-core && npm run build-http && npm run build-router && node .config/build.js",
    "build-core": "ng-packagr -p lib/core/ng-package.json",
    "build-http": "ng-packagr -p lib/http/ng-package.json",
    "build-router": "ng-packagr -p lib/router/ng-package.json",
    "publish-all": "node .config/publish.js",
    "version": "node .config/build.js",
    "link-core": "npm run version && npm link build/core && npm link @ngx-progressbar/core",
    "link-http": "npm run version && npm link build/http && npm link @ngx-progressbar/http",
    "link-router": "npm run version && npm link build/router && npm link @ngx-progressbar/router"
  },
  "private": false,
  "homepage": "http://github.com/murhafsousli/ngx-progressbar",
  "author": {
    "name": "Murhaf Sousli",
    "url": "http://github.com/murhafsousli"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/murhafsousli/ngx-progressbar.git"
  },
  "keywords": [
    "angular",
    "progress",
    "progressbar",
    "loading",
    "http",
    "xhr",
    "interceptors",
    "loader",
    "slim",
    "youtube",
    "medium",
    "github"
  ],
  "bugs": {
    "url": "http://github.com/murhafsousli/ngx-progressbar/issues"
  },
  "license": "MIT",
  "dependencies": {
    "@angular/animations": "^5.2.0",
    "@angular/cdk": "^5.0.4",
    "@angular/common": "^5.2.0",
    "@angular/compiler": "^5.2.0",
    "@angular/core": "^5.2.0",
    "@angular/forms": "^5.2.0",
    "@angular/material": "^5.0.4",
    "@angular/platform-browser": "^5.2.0",
    "@angular/platform-browser-dynamic": "^5.2.0",
    "@angular/router": "^5.2.0",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.4.1",
    "github-fork-ribbon-css": "^0.2.1",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.14"
  },
  "devDependencies": {
    "@angular/cli": "^1.7.4",
    "@angular/compiler-cli": "^5.2.0",
    "@angular/language-service": "^5.2.0",
    "@types/jasmine": "~2.5.53",
    "@types/jasminewd2": "~2.0.2",
    "@types/node": "~6.0.60",
    "codelyzer": "^4.0.1",
    "jasmine-core": "~2.6.2",
    "jasmine-spec-reporter": "~4.1.0",
    "karma": "~1.7.0",
    "karma-chrome-launcher": "~2.1.1",
    "karma-cli": "~1.0.1",
    "karma-coverage-istanbul-reporter": "^1.2.1",
    "karma-jasmine": "~1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "ng-packagr": "^2.4.2",
    "protractor": "~5.1.2",
    "standard-version": "^4.2.0",
    "ts-node": "~3.2.0",
    "tsickle": "^0.25.6",
    "tslint": "~5.7.0",
    "typescript": "~2.4.2"
  }
}
